  "private": true,
  "publishConfig": {
    "registry": "http://www.cagst.com:8081/repository/cagst-npm/"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ngx-configuration": "ng build ngx-configuration && cd dist && npm pack ngx-configuration/",
